{
    "name": "juling/laravel-operate-log",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.2",
        "alibabacloud/aliyun-log-php-sdk": "^0.6.3",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\OperateLog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\OperateLog\\OperateLogServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
