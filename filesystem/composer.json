{
    "name": "juling/laravel-filesystem",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.3",
        "aliyuncs/oss-sdk-php": "^2.7",
        "laravel/framework": "^12.0",
        "overtrue/laravel-filesystem-cos": "^3.5",
        "overtrue/laravel-filesystem-qiniu": "^2.3"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\Filesystem\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\Filesystem\\FilesystemServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
