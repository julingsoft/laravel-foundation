{
    "name": "juling/laravel-pinyin",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.3",
        "overtrue/laravel-pinyin": "^5.1",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\Pinyin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\Pinyin\\PinyinServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
