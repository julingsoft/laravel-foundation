{
    "name": "juling/laravel-qr-code",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.2",
        "endroid/qr-code": "^5.0",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\QrCode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\QrCode\\QrCodeServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
