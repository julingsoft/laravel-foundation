{
    "name": "juling/laravel-pusher",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.2",
        "workerman/crontab": "^1.0",
        "workerman/gateway-worker": "^3.1",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Juling\\Pusher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juling\\Pusher\\PusherServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
